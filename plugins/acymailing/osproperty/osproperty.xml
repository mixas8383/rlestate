<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install SYSTEM "http://dev.joomla.org/xml/1.5/plugin-install.dtd">
<install type="plugin" version="1.5" method="upgrade" group="acymailing">
	<name>AcyMailing Tag : OS Property integration</name>
	<creationDate>Aug 2016</creationDate>
	<version>3.0.1</version>
	<author>Ossolution</author>
	<authorEmail>damdt@joomdonation.com</authorEmail>
	<authorUrl>http://www.joomservices.com</authorUrl>
	<copyright>Copyright (C) 2011-2016 Ossolution - All rights reserved..</copyright>
	<license>GNU/GPLv3 http://www.gnu.org/licenses/gpl-3.0.html</license>
	<description>This AcyMailing plugin enables you to include propertues of OS Property extension in AcyMailing</description>
	<files>
		<filename plugin="osproperty">osproperty.php</filename>
		<filename>index.html</filename>
	</files>
	<params addpath="/components/com_acymailing/params">
	</params>
	<config>
		<fields name="params" addfieldpath="/components/com_acymailing/params">
			<fieldset name="basic">
			</fieldset>
		</fields>
	</config>
</install>
