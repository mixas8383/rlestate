<?xml version="1.0" encoding="utf-8"?>
<extension type="module" client="administrator" method="upgrade" version="2.5" position="icon">
    <name>OS Property Quick Icons (admin)</name>
    <author>Dang Thuc Dam</author>
    <creationDate>March 2015</creationDate>
    <copyright>Copyright (c) 2007 - 2015 Joomdonation.com. All rights reserved.</copyright>
    <authorEmail>damdt@joomservices.com</authorEmail>
    <authorUrl>www.joomdonation.com</authorUrl>
    <version>2.7.5</version>
    <description>OS Property quickicons for use in the Joomla! control panel (dashboard) page</description>
    <files>
        <filename module="mod_osquickicons">mod_osquickicons.php</filename>
		<filename>helper.php</filename>
        <filename>index.html</filename>
        <folder>tmpl</folder>
    </files>
	<languages>
		<language tag="en-GB">en-GB.mod_osquickicons.ini</language>
		<language tag="en-GB">en-GB.mod_osquickicons.sys.ini</language>
	</languages>
    <config>
        <fields name="params">
            <fieldset name="basic">
                <field name="modLogo" type="list" default="1" label="OS_MODULE_LOGO" description="">
                    <option value="1">YES</option>
                    <option value="0">NO</option>
                </field>
            </fieldset>
        </fields>
    </config>
</extension>
